<!DOCTYPE html>
<html>
	<head>
		<title><%=title %></title>
		<link rel='stylesheet' href='/stylesheets/photo.css'>
	</head>
	<body>
		<h1>Photos</h1>
		<p>Express stock photo application</p>

		<div id="photos">
			<% photos.forEach(function(photo){ %>
				<div class="photo">
					<h2><%=photo.name %></h2>
					<a href='/photo/<%=photo.id%>/download' title='Click Download This Photo'>
						<img src="/photos/<%=photo.path %>"  >
					</a>
				</div>
			<% }) %>
		</div>
		<div>
			<a id="upload" href="/upload">Go Upload</a> 
		</div>
	</body>
</html>